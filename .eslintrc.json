{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "react",
    "jsx-a11y"
  ],
  "globals": {
    "M": true,
    "Translator": true,
    "Routing": true
  },
  "parser": "babel-eslint",
  "rules": {
    "jsx-a11y/label-has-for": [ 2, {
      "required": {
        "some": [ "nesting", "id" ]
      }
    }],
    "no-param-reassign": [ "error", {
      "props": false
    }]
  }
}
